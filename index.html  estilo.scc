h1 {
    font-size: 30PX;
    font-family: sans-serif;
    font-weight: bold;
}
h2 {
    font-weight: bold;
    font-size: 20px;
    font-family: sans-serif;
}

header,section,footer, aside, nav, article {
    display: block;
}

body {
    text-align: center;
    background: black;
}
.agrupar {
    width: 960px;
    border: 1px solid black;
    margin: 15px auto;
    background: white;
}
.cabecera {
    background: #a7a7a7;
    border: 1px solid black;
    padding: 1px;
}
.cabecera h1 {
    text-align: left;
}

.menu {
    background: #8fe3ff;
    padding:  1px;
    text-align: left;
}

.menu li {
    list-style: none;
    padding: 1px;
    font: bold 14px verdana;
    display: inline-block;
}

.seccion {
    margin: 20px;
    width: 600px;
    float: left;
}

time {
    color:blue;
}

article {
    background: #ffdccb;
    border: 1px solid #999;
    margin-bottom: 15px;
}

article footer {
    text-align: right;
    margin-top: 20px;
}

.columna {
    width: 300px;
    float: left;
    margin: 20px 0;
}

.columna blockquote {
    border: 1px solid black;
    padding: 40px;
    background: #ffd5c8;
    margin-bottom: 15px;
}

.pie {
    clear: both;
    padding: 15px;
    background: #a7a7a7;
}

img {
    width: 120px;
    display: block;
    margin: 20px auto;
}
